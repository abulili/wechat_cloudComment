<van-cell-group>
  <van-row>
  <van-col span="17" class="userImgTxt"><van-cell title="用户头像"/> </van-col>
  <van-col span="7"> 
  <!-- <image mode="aspectFill" lazy-load src="https://z3.ax1x.com/2021/03/31/ckVNT0.jpg">
  </image> -->
  <button class="avatar-wrapper" open-type="chooseAvatar" bind:chooseavatar="onChooseAvatar">
    <image class="avatar" mode="aspectFill" lazy-load src="{{avatarUrl}}"></image>
  </button> 
  </van-col>
  </van-row>
  <van-cell title="用户名"><input type="nickname" class="weui-input" value="{{username}}" bindblur="updateName"/></van-cell>
  <van-cell title="我的贴子" value="点击查看" border="{{ false }}" bindtap="myHistory"/>
  <van-cell title="去吐槽" border="{{ false }}" />
</van-cell-group>
<button bindtap="btn">{{loginStatus}}</button>



